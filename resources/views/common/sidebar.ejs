                            <li><a href="/register">Sign Up <i class="fas fa-chevron-right"></i></a></li>
            </ul>

            <% if(admin==='admin' || admin==='super_admin' ) { %>
                <div id="hr"></div>
                <ul style="padding-left:0;">
                    <h5 style="color:#000;">Dashboard</h5>
                    <li> <a href="/api/adminPage/dashboard">Admin Dashboard<i class="fas fa-chevron-right"></i></a></li>
                </ul>
