                    <ul id="lapContent" class="lapContent" style="padding-left:0;">

                        <!-- Asus Laptops -->

                        <li>
                            <a id="asuslap" data-productid="<%= asuslap ? JSON.stringify(asuslap) : 'Asuslap' %>"
